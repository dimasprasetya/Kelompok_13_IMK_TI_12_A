                          android:layout_alignParentTop="true" android:layout_toRightOf="@+id/imageView2"/>
                <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                          android:textAppearance="?android:attr/textAppearanceLarge" android:text="Bandarlampung"
                          android:id="@+id/textView5" android:textColor="#000000" android:textSize="20sp"
                          android:layout_alignParentBottom="true" android:layout_toRightOf="@+id/imageView2"/>
                <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                          android:textAppearance="?android:attr/textAppearanceLarge" android:text="S1 - Teknik Informatika"
