                          android:layout_centerVertical="true" android:layout_toRightOf="@+id/imageView2"/>
            </RelativeLayout>
            <RelativeLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="100dp"
                          android:layout_gravity="center_horizontal|top" android:background="@drawable/button_linier" android:id="@+id/linearLayout"
                          android:layout_below="@+id/relativeLayout" android:layout_alignParentLeft="true"
                          android:layout_marginTop="5dp" android:weightSum="1">
                <ImageView
                        android:layout_width="100dp"
                        android:layout_height="100dp"
                        android:id="@+id/imageView" android:background="@drawable/pp1" android:layout_alignParentLeft="true"
                        android:layout_marginLeft="0dp" android:layout_alignParentTop="true" android:layout_marginTop="0dp"/>
                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Nama"
                        android:id="@+id/textView"
                        android:textColor="#000000" android:textSize="20sp"
                        android:layout_alignParentTop="true" android:layout_toRightOf="@+id/imageView"/>
                <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                          android:textAppearance="?android:attr/textAppearanceLarge" android:text="Alamat"
                          android:id="@+id/textView2" android:textColor="#000000" android:textSize="20sp"
                          android:layout_alignParentBottom="true" android:layout_toRightOf="@+id/imageView"/>
                <TextView android:layout_width="wrap_content" android:layout_height="wrap_content"
                          android:textAppearance="?android:attr/textAppearanceLarge" android:text="Pendidikan Terakhir"
                          android:id="@+id/textView3" android:textColor="#000000" android:textSize="20sp"
                          android:layout_centerVertical="true" android:layout_toRightOf="@+id/imageView"/>
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
    <RelativeLayout android:layout_width="fill_parent" android:layout_height="66dp"
                    android:layout_gravity="center_horizontal|top" android:background="@android:color/background_light"
                    android:orientation="horizontal" android:layout_alignParentLeft="true"
                    android:layout_marginLeft="0dp" android:id="@+id/relativeLayout"
                    android:layout_alignParentTop="true">
        <Button android:layout_width="33dp" android:layout_height="33dp" android:id="@+id/buttonHome"
                android:layout_gravity="right" android:background="@drawable/buton_home"
                android:layout_alignParentLeft="true" android:layout_marginLeft="3dp"
                android:layout_alignParentTop="true" android:layout_marginTop="3dp"/>
        <SearchView android:layout_width="wrap_content" android:layout_height="wrap_content"
                    android:id="@+id/searchView2"
                    android:layout_below="@+id/textView19" android:layout_alignParentRight="true"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:id="@+id/textView19" android:layout_alignTop="@+id/buttonHome"
                android:layout_centerHorizontal="true" android:textColor="#020001" android:textSize="20sp"
                android:text="Pencari Kerja" android:textStyle="bold|italic"/>
    </RelativeLayout>
</RelativeLayout>